{
  "name": "church-app-frontend",
  "version": "1.0.0",
  "description": "Frontend web application for Church Management Application - Sing Buri Adventist Center",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/inject-sw-version.mjs",
    "prebuild": "node -e \"console.log('Build started at:', new Date().toISOString())\"",
    "postbuild": "node -e \"console.log('Build completed at:', new Date().toISOString())\"",
    "preview": "vite preview",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit"
